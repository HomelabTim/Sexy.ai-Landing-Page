{# 01 - Layout #}

<!DOCTYPE html>
<html lang="{{ config.site.locale.language }}-{{ config.site.locale.region }}">
    <head>
        <title>{{ config.site.title }}</title>

        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />

        <meta name="title" content="{{ config.site.title }}" />
        <meta name="description" content="{{ config.site.description }}" />
        <meta name="keywords" content="{{ config.site.keywords.primary }}" />
        <meta name="url" content="{{ config.domain.canonical }}/{{ page.inputPath | fileName }}" />

        <meta name="theme-color" content="{{ config.site.theme }}" />
        <meta name="color-scheme" content="dark" />

        <meta property="og:type" content="{{ config.site.social.openGraph.type }}" />
        <meta property="og:title" content="{{ config.site.social.openGraph.title }}" />
        <meta property="og:description" content="{{ config.site.social.openGraph.description }}" />
        <meta property="og:url" content="{{ config.site.social.openGraph.url }}" />
        <meta property="og:locale" content="{{ config.site.locale.language }}-{{ config.site.locale.region }}" />
        <meta property="og:image" content="{{ config.site.social.openGraph.image.url }}" />
        <meta property="og:image:alt" content="{{ config.site.social.openGraph.image.alt }}" />
        <meta property="og:image:width" content="64" />
        <meta property="og:image:height" content="64" />

        <meta name="twitter:card" content="{{ config.site.social.twitter.card }}" />
        <meta name="twitter:site" content="{{ config.site.social.twitter.username }}" />
        <meta name="twitter:title" content="{{ config.site.social.twitter.title }}" />
        <meta name="twitter:description" content="{{ config.site.social.twitter.description }}" />
        <meta name="twitter:image" content="{{ config.site.social.twitter.image.url }}" />
        <meta name="twitter:image:alt" content="{{ config.site.social.twitter.image.alt }}" />
        <meta property="twitter:image:width" content="64" />
        <meta property="twitter:image:height" content="64" />

        <meta name="robots" content="noarchive" />

        <link href="{{ config.domain.canonical }}/{{ page.inputPath | fileName }}" rel="canonical"  />
        <link href="{{ config.site.favicon }}" rel="icon" type="image/x-icon" />

		<link href="/assets/css/bootstrap.min.css" rel="stylesheet" />
		<link href="/assets/css/font-awesome.min.css" rel="stylesheet" />
		<link href="/assets/css/animate.min.css" rel="stylesheet" />
		<link href="/assets/css/prettyphoto.css" rel="stylesheet" />
		<link href="/assets/css/styles.css" rel="stylesheet" />
		<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" rel="stylesheet" />

		<!--[if lt IE 9]>
			<script src="/assets/js/html5shiv.js"></script>
			<script src="/assets/js/respond.min.js"></script>
		<![endif]-->
		<link rel="shortcut icon" href="https://files.cdn2.io/images/logos/logo.jpg" />
    </head>

    <body id="home">
		<header id="header">
			<nav
				id="main-nav"
				class="navbar navbar-default navbar-fixed-top"
				role="banner"
			>
				<div class="navbar-header">
					<a href="/"
						><img
							src="https://files.cdn2.io/images/logos/logo-gold-dark.webp"
							alt="logo"
					/></a>

					<button
						type="button"
						class="navbar-toggle"
						data-toggle="collapse"
						data-target=".navbar-collapse"
					>
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
				</div>

				<div class="collapse navbar-collapse navbar-right">
					<ul class="nav navbar-nav">
						<li class="scroll active">
							<a href="#home">Home</a>
						</li>
						<li class="scroll">
							<a href="https://sexy.ai/sign-in">Sign In</a>
						</li>
						<li class="scroll">
							<a href="https://sexy.ai/">Generate</a>
						</li>
						<li class="scroll">
							<a href="/#pricing">Pricing</a>
						</li>
						<li class="scroll">
							<a href="/#contact-us">Contact</a>
						</li>
					</ul>
				</div>
			</nav>
		</header>

        {{ content | safe }}
        
		<footer id="footer">
			<div class="container">
				<div class="row">
					<div class="footer-text">
						&copy; 2024 sexy.ai. All rights reserved |
						<a href="/2257-compliance.html"
							>2257 Compliance Notice</a
						>
						|
						<a href="/privacy-policy.html">Privacy Policy</a>
						|
						<a href="/terms.html">Terms of Service</a>
						|
						<a href="/contact.html">Contact Us</a>
					</div>
				</div>
			</div>
		</footer>

		<script src="/assets/js/jquery.js"></script>
		<script src="/assets/js/bootstrap.min.js"></script>
		<script src="/assets/js/mousescroll.js"></script>
		<script src="/assets/js/smoothscroll.js"></script>
		<script src="/assets/js/jquery.prettyphoto.js"></script>
		<script src="/assets/js/jquery.isotope.min.js"></script>
		<script src="/assets/js/jquery.inview.min.js"></script>
		<script src="/assets/js/wow.min.js"></script>
		<script src="/assets/js/custom-scripts.js"></script>
		<script src="/assets/js/script.js"></script>
    </body>
</html>
